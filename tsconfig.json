{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "resolveJsonModule": true
    },
    "lib": ["es2015"],
    "rules": {
        "no-console": false
    },
    "rulesDirectory": [],
    "include":[ 
      "src/**/*",
      "src/**/*.json",
    ],
  }